{
  "name": "tomcizek/prooph-response-recorder",
  "type": "library",
  "description": "Evil tool for prooph when you want to temporary hide your synchronous CQRS backend behind request/response interface to use in legacy REST or WebApp controllers.",
  "license": "MIT",
  "authors": [
    {
      "name": "Tomáš Čížek",
      "email": "tomascizek@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "prooph/event-sourcing": "^5.2",
    "prooph/service-bus": "^6.1",
    "psr/container": "^1.0",
    "psr/log": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "TomCizek\\ResponseRecorder\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "phpstan/phpstan": "^0.8.5"
  },
  "autoload-dev": {
    "psr-4": {
      "TomCizek\\ResponseRecorder\\Tests\\": "tests"
    }
  }
}
